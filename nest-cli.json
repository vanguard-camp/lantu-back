{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/lantu-back/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/lantu-back/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/lantu-back",
  "projects": {
    "lantu-back": {
      "type": "application",
      "root": "apps/lantu-back",
      "entryFile": "main",
      "sourceRoot": "apps/lantu-back/src",
      "compilerOptions": {
        "tsConfigPath": "apps/lantu-back/tsconfig.app.json"
      }
    },
    "devops": {
      "type": "application",
      "root": "apps/devops",
      "entryFile": "main",
      "sourceRoot": "apps/devops/src",
      "compilerOptions": {
        "tsConfigPath": "apps/devops/tsconfig.app.json"
      }
    },
    "comm": {
      "type": "library",
      "root": "libs/comm",
      "entryFile": "index",
      "sourceRoot": "libs/comm/src",
      "compilerOptions": {
        "tsConfigPath": "libs/comm/tsconfig.lib.json"
      }
    }
  }
}